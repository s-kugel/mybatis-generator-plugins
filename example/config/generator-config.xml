<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context
    id="generator-db"
    defaultModelType="hierarchical"
    targetRuntime="MyBatis3"
  >
    <property
      name="javaFileEncoding"
      value="UTF-8"
    />
    <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
    <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin" />
    <!-- Custom Generator Plugins -->
    <plugin type="org.mybatis.generator.plugins.LombokPlugin" />
    <plugin type="org.mybatis.generator.plugins.SelectiveSQLPlugin" />
    <plugin type="org.mybatis.generator.plugins.VersionColumnPlugin" />
    <plugin type="org.mybatis.generator.plugins.TruncateSQLPlugin" />
    <plugin type="org.mybatis.generator.plugins.SelectAllPlugin" />
    <plugin type="org.mybatis.generator.plugins.BulkInsertPlugin" />
    <plugin type="org.mybatis.generator.plugins.SpecificColumnSQLPlugin" />
    <commentGenerator>
      <property
        name="suppressDate"
        value="true"
      />
    </commentGenerator>
    <jdbcConnection
      driverClass="${driverClassName}"
      connectionURL="${url}"
      userId="${username}"
      password="${password}"
    />
    <javaTypeResolver>
      <property
        name="forceBigDecimals"
        value="true"
      />
      <property
        name="useJSR310Types"
        value="true"
      />
    </javaTypeResolver>
    <javaModelGenerator
      targetPackage="${targetEntityPackage}"
      targetProject="${targetProject}"
    />
    <sqlMapGenerator
      targetPackage="${targetMapperPackage}"
      targetProject="${targetProject}"
    />
    <javaClientGenerator
      type="ANNOTATEDMAPPER"
      targetPackage="${targetMapperPackage}"
      targetProject="${targetProject}"
    />
    <!-- tables -->
    <table
      tableName="user"
      domainObjectName="system.gen.User"
      mapperName="system.gen.UserMapperDefault"
      sqlProviderName="system.gen.UserSqlProviderDefault"
      modelType="flat"
      enableSelectByExample="false"
      enableDeleteByExample="false"
      enableCountByExample="false"
      enableUpdateByExample="false"
    />
    <table
      tableName="execution"
      domainObjectName="trade.gen.Execution"
      mapperName="trade.gen.ExecutionMapperDefault"
      sqlProviderName="trade.gen.ExecutionSqlProviderDefault"
      modelType="flat"
      enableSelectByExample="false"
      enableDeleteByExample="false"
      enableCountByExample="false"
      enableUpdateByExample="false"
    />
  </context>
</generatorConfiguration>
